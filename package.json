    "@tanstack/react-table": "^8.11.2",
    "auto-zustand-selectors-hook": "^2.0.0",
    "axios": "1.6.3",
    "clsx": "^2.0.0",
    "immer": "^9.0.21",
    "lodash.get": "^4.4.2",
    "lucide-react": "^0.294.0",
    "next": "^14.0.4",
    "nprogress": "^0.2.0",
    "query-string": "^8.1.0",
